<%- include('../partials/html-head') %>
<link rel="stylesheet" href="/stylesheets/polls/index.css">
<%- include('../partials/nav') %>

<main>
  <h1><%= title %></h1>
  <div id="polls-container">
    <% poll.forEach(poll => { %>
      <div>
        <a href="/polls/<%= poll._id %>"><h2><%= poll.question %></h2></a>
      </div>
    <% }) %>
  </div>
</main>

