  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  <header>
    <a  href="/"><img id="icon" src="/stylesheets/images/iconCM.png" alt=""></a>
    <nav>
      <a id="top-nav"
        href="/" <%- title === "Profile" ? 'class="active"' : "" %>
        >
        Home
      </a>
      
      <% if (user) { %>

        <a id="top-nav"
          href="/profiles" <%- title === "Profile" ? 'class="active"' : "" %>
          >
          Profile
        </a>
        <a id="top-nav"
          href="/profiles" <%- title === "Favorites" ? 'class="active"' : "" %>
          >
          Favorites
        </a>
      <% } %>
      <a id="top-nav"
        href="/polls" <%- title === "Favorites" ? 'class="active"' : "" %>
        >
      All Polls
      </a>
      <a id="top-nav"
        href="/polls/new" <%- title === "Favorites" ? 'class="active"' : "" %>
        >
        Create Poll
      </a>
    </nav>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
    <% } else { %>
      <a href="/login">
        <button>Login</button>
      </a>
    <% } %>
  </header>
